<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    	<class name="com.kirti.foodplaza.pojo.Customer" table="customer_22063">
    	<id name="custId" column="cust_id"><!-- column="food_id" -->
    		<generator class="identity"></generator>
    	</id>
    	<property name="custEmail" column="cust_email"/><!--  column="food_name" -->
    	 <property name="custPassword" column="cust_password"/>
    	<property name="contactNumber" column="contact_number"/><!-- column="food_price" -->
    	<property name="address" column="address"/>
    	</class>
    
    </hibernate-mapping>
